   "source": [
    "foot_size = int(input())\n",
    "sizes = sorted([int(x) for x in input().split()])\n",
    "\n",
    "count = 0\n",
    "previous_suitable_size = foot_size - 3\n",
    "\n",
    "for size in sizes:\n",
    "    if size >= previous_suitable_size + 3:\n",
    "        previous_suitable_size = size\n",
    "        count += 1\n",
    "print(count)"
   ]
  }
