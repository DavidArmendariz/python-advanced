    "N = int(input())\n",
    "words = dict()\n",
    "for n in range(N):\n",
    "    word = input()\n",
    "    length = len(word)\n",
    "    words[length] = words.get(length, []) + [word]\n",
    "for length in sorted(words):\n",
    "    for word in sorted([x[::-1] for x in words[length]]):\n",
    "        print(word[::-1])"
