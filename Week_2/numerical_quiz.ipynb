   "source": [
    "n = int(input())\n",
    "words = [input() for _ in range(n)]\n",
    "def decompose(x):\n",
    "    return sum([int(x[i])-int(x[len(x)-i-1]) for i in range(len(x)//2)])\n",
    "words.sort(key=lambda word: (decompose(word), abs(int(word))))\n",
    "print(*words, sep=\"\\n\")"
   ]
  }
