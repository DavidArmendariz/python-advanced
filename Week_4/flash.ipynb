    "class Flash:\n",
    "    def __init__(self, capacity):\n",
    "        self.capacity = capacity\n",
    "        self.size = 0\n",
    "\n",
    "    def write(self, v):\n",
    "        if v + self.size > self.capacity:\n",
    "            raise ValueError\n",
    "        else:\n",
    "            self.size += v\n"
   ]
